package com.kh.test.main;

import java.util.ArrayList;
import java.util.Arrays;

public class 리스트예제 {

	public static void main(String[] args) {
		ArrayList<String> list = new ArrayList<>(Arrays.asList("바나나", "오렌지"));
		list.add("사과");
		
		System.out.println(list);
		
		list.remove("바나나");
		list.remove(0);
	}
}
