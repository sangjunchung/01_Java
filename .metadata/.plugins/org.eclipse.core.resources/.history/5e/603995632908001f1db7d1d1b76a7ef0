package com.kh.hamburger;

import java.util.ArrayList;

public class BurgerShop {

	public static void main(String[] args) {
		ArrayList<HamburgerOrder> list = new ArrayList<>();
		list.add(new HamburgerOrder("동그라미", "치즈버거"));
		list.add(new HamburgerOrder("안세모", "베이컨버거"));
		list.add(new HamburgerOrder("박네모", "상하이버거"));
		list.add(new HamburgerOrder("금각진", "치킨버거"));
		list.add(new HamburgerOrder("동그라미", "빅맥"));
		
		System.out.println(list);
	}

}
