package com.kh.sandwich;

public class Sandwich {
	private String clientName;
	private String sandwichType;

	public void setClientName(String clientName) {
		this.clientName = clientName;
	}

	public void setSandwichType(String sandwichType) {
		this.sandwichType = sandwichType;
	}

	public String getClientName() {
		return clientName;
	}

	public String getSandwichType() {
		return sandwichType;
	}

	public Sandwich(String clientName, String sandwichType) {
		this.clientName = clientName;
		this.sandwichType = sandwichType;
	}

	@Override
	public String toString() {
		return clientName + " " + sandwichType;
	}

	
}
